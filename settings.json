{
  "env": {
    "DISABLE_AUTOUPDATER": "1"
  },
  "permissions": {
    "allow": [
      "Bash(cd:*)",
      "Bash(ls:*)",
      "Bash(grep:*)",
      "Bash(cat:*)",
      "Bash(pwd)",
      "Bash(echo:*)",
      "Bash(git status:*)",
      "Bash(git diff:*)",
      "Bash(git log:*)",
      "Bash(wc:*)"
    ]
  },
  "enabledPlugins": {
    "llm-rules@Meta": true,
    "meta@Meta": true
  },
  "alwaysThinkingEnabled": false
}
